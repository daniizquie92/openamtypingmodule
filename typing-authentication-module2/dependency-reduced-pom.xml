<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <parent>
    <artifactId>openam</artifactId>
    <groupId>org.openidentityplatform.openam</groupId>
    <version>14.6.7-SNAPSHOT</version>
    <relativePath>../pom.xml/pom.xml</relativePath>
  </parent>
  <modelVersion>4.0.0</modelVersion>
  <groupId>org.openidentityplatform.openam.examples</groupId>
  <artifactId>typing-authentication-module</artifactId>
  <name>OpenAM Custom Authentication Module</name>
  <build>
    <plugins>
      <plugin>
        <artifactId>maven-shade-plugin</artifactId>
        <version>3.4.1</version>
        <executions>
          <execution>
            <phase>package</phase>
            <goals>
              <goal>shade</goal>
            </goals>
          </execution>
        </executions>
      </plugin>
    </plugins>
  </build>
  <dependencies>
    <dependency>
      <groupId>javax.servlet</groupId>
      <artifactId>servlet-api</artifactId>
      <version>2.5</version>
      <scope>provided</scope>
    </dependency>
    <dependency>
      <groupId>org.forgerock.openam</groupId>
      <artifactId>openam-core</artifactId>
      <version>7.2.0</version>
      <scope>system</scope>
      <systemPath>${project.basedir}/lib/openam-core-14.6.6.jar</systemPath>
    </dependency>
    <dependency>
      <groupId>org.forgerock.openam</groupId>
      <artifactId>openam-shared</artifactId>
      <version>7.2.0</version>
      <scope>system</scope>
      <systemPath>${project.basedir}/lib/openam-shared-14.6.6.jar</systemPath>
    </dependency>
    <dependency>
      <groupId>com.googlecode.json-simple</groupId>
      <artifactId>json-simple</artifactId>
      <version>1.1</version>
      <scope>system</scope>
      <systemPath>${project.basedir}/lib/json-simple-1.1.jar</systemPath>
    </dependency>
    <dependency>
      <groupId>nl.captcha</groupId>
      <artifactId>simplecaptcha</artifactId>
      <version>2.0</version>
      <scope>system</scope>
      <systemPath>${project.basedir}/lib/simplecaptcha-1.2.jar</systemPath>
    </dependency>
  </dependencies>
  <properties>
    <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
  </properties>
</project>
